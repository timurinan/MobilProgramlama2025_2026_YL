<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/frameLayout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".HesapFragment">

    <TextView
        android:id="@+id/txt_bakiye"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginStart="56dp"
        android:layout_marginEnd="56dp"
        android:layout_marginBottom="304dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView5" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Bakiye"
        app:layout_constraintBottom_toTopOf="@+id/txt_bakiye"
        app:layout_constraintStart_toStartOf="@+id/txt_bakiye"
        app:layout_constraintTop_toBottomOf="@+id/txt_hesaptürü" />

    <TextView
        android:id="@+id/txt_hesaptürü"
        android:layout_width="287dp"
        android:layout_height="0dp"
        android:layout_marginBottom="42dp"
        app:layout_constraintBottom_toTopOf="@+id/textView5"
        app:layout_constraintStart_toStartOf="@+id/textView5"
        app:layout_constraintTop_toBottomOf="@+id/textView3" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Hesap Türü"
        app:layout_constraintBottom_toTopOf="@+id/txt_hesaptürü"
        app:layout_constraintStart_toStartOf="@+id/txt_hesaptürü"
        app:layout_constraintTop_toBottomOf="@+id/txt_hesapsahibi" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="192dp"
        android:text="Hesap Sahibi"
        app:layout_constraintBottom_toTopOf="@+id/txt_hesapsahibi"
        app:layout_constraintStart_toStartOf="@+id/txt_hesapsahibi"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/txt_hesapsahibi"
        android:layout_width="287dp"
        android:layout_height="0dp"
        android:layout_marginBottom="34dp"
        app:layout_constraintBottom_toTopOf="@+id/textView3"
        app:layout_constraintStart_toStartOf="@+id/textView3"
        app:layout_constraintTop_toBottomOf="@+id/textView" />
</androidx.constraintlayout.widget.ConstraintLayout>